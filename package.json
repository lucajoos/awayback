{
  "name": "onceupon.js",
  "version": "1.5.11",
  "description": "Custom event system for JavaScript exported as Node.js module.",
  "repository": "https://github.com/lucajoos/onceupon.js",
  "exports": {
    ".": {
      "types": "./types/onceupon.d.ts",
      "import": "./esm/onceupon.js",
      "require": "./cjs/onceupon.js"
    }
  },
  "main": "./cjs/onceupon.js",
  "types": "./types/onceupon.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc --project ./tsconfig.cjs.json",
    "watch": "(tsc --project tsconfig.json --watch &) && tsc --project ./tsconfig.cjs.json --watch"
  },
  "keywords": [
    "onceupon.js",
    "events"
  ],
  "author": {
    "name": "Luca Joos",
    "email": "1a85ra7z@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.6.3",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  },
  "bugs": "https://github.com/lucajoos/onceupon.js/issues"
}
